        <definition>uint32_t pros::c::link_transmit_raw</definition>
        <argsstring>(uint8_t port, void *data, uint16_t data_size)</argsstring>
        <name>link_transmit_raw</name>
        <param>
          <type>uint8_t</type>
          <declname>port</declname>
        </param>
        <param>
          <type>void *</type>
          <declname>data</declname>
        </param>
        <param>
          <type>uint16_t</type>
